{
  "name": "rbacjs",
  "version": "2.0.1",
  "description": "Role based access control.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test:development": "jest",
    "test": "jest && codecov",
    "build": "webpack",
    "prepublish": "jest"
  },
  "author": "Alexandr Zahorovskyi <zahorovskiy.a.a@gmail.com> (https://github.com/zahorovskyi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zahorovskyi/rbacjs/issues"
  },
  "homepage": "https://github.com/zahorovskyi/rbacjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/zahorovskyi/rbacjs.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "codecov": "^3.6.1",
    "jest": "^25.0.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-loader-decleration": "^0.10.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {},
  "jest": {
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.(ts|tsx|js)$"
  },
  "keywords": [
    "rbac",
    "rbacjs",
    "role base access control"
  ]
